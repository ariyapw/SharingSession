import { Override, Data, Color } from "framer"

export function Tombolnyala(): Override {
    return {
        onTap: () => {
            fetch(
                "https://maker.ifttt.com/trigger/nyala_lampu/with/key/dAn5zxdeMoaP25aNI74f-mq0W282bUtSBvzGCkgwaRN",
                { method: "post" }
            )
        },
    }
}

